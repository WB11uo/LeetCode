# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class Solution:
    def lowestCommonAncestor(self, root: 'TreeNode', p: 'TreeNode', q: 'TreeNode') -> 'TreeNode':
        anc = root
        while True:
            if p.val < anc.val and q.val < anc.val:
                anc = anc.left
            elif p.val > anc.val and q.val > anc.val:
                anc = anc.right
            else:
                break
        return anc
